{
  "name": "restifizer",
  "version": "0.4.21",
  "description": "Restful database-agnostic service",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-migrate && npm run test-app | npm run test-cucumber && npm run test-migrate-undo",
    "test-clear": "npm run test-migrate-undo",
    "test-app": "node ./test/lib/app/server.js",
    "test-cucumber": "cd ./test && echo \"Wait 5 seconds (test server starting)...\" && sleep 5 && cucumber.js",
    "test-migrate": "cd ./test/lib/app/ && ../../../node_modules/.bin/sequelize db:migrate",
    "test-migrate-undo": "cd ./test/lib/app/ && ../../../node_modules/.bin/sequelize db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vedi/restifizer.git"
  },
  "keywords": [
    "node.js",
    "rest",
    "restful",
    "express",
    "node",
    "mongo",
    "mongodb",
    "mongoose",
    "nosql"
  ],
